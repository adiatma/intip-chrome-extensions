{
  "name": "Intip - Chrome extension",
  "description": "Chrome extension to get ip information",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "16": "icons/16.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/original.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      }
    }
  }
}
